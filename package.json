{
  "name": "lightwalker-mvp",
  "version": "0.1.1",
  "private": true,
  "description": "Lightwalker MVP - Ideal Self Twin Character System",
  "author": "Life Designer Team",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=18.17.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev -p 3001",
    "dev:clean": "npm run clean && next dev -p 3001",
    "dev:safe": "npm run kill-port && npm run dev:clean",
    "dev:full": "npm run kill-port && npm run clean && npm run db:generate && next dev -p 3001",
    "kill-port": "taskkill /F /IM node.exe 2>nul || echo 'No node processes running'",
    "build": "prisma generate && next build",
    "build:clean": "npm run clean && npm run build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "postinstall": "prisma generate",
    "clean": "rm -rf .next out dist build",
    "reset": "npm run clean && npm install && npm run db:generate"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@next/font": "^14.0.4",
    "@prisma/client": "^5.8.1",
    "@t3-oss/env-nextjs": "^0.7.1",
    "@tanstack/react-query": "^5.17.15",
    "@trpc/client": "^10.45.0",
    "@trpc/next": "^10.45.0",
    "@trpc/react-query": "^10.45.0",
    "@trpc/server": "^10.45.0",
    "clsx": "^2.1.0",
    "iron-session": "^8.0.1",
    "jsonwebtoken": "^9.0.2",
    "next": "14.2.35",
    "openai": "^4.24.7",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "superjson": "^2.2.1",
    "tailwind-merge": "^2.2.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@playwright/test": "^1.40.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "autoprefixer": "^10.4.16",
    "class-variance-authority": "^0.7.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lucide-react": "^0.303.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "prisma": "^5.8.1",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
